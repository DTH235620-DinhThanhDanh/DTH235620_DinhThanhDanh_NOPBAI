   "source": [
    "Yêu cầu:\n",
    "Cho list\n",
    "lst = [20, 1, -34, 40, -8, 60, 1, 3]\n",
    "Hãy cho biết kết quả:\n",
    "(a) lst\n",
    "(b) lst[0:3]\n",
    "(c) lst[4:8]\n",
    "Trang 55/84\n",
    "(d) lst[4:33]\n",
    "(e) lst[-5:-3]\n",
    "(f) lst[-22:3]\n",
    "(g) lst[4:]\n",
    "(h) lst[:]\n",
    "(i) lst[:4]\n",
    "(j) lst[1:5]\n",
    "(k) -34 in lst\n",
    "(l) -34 not in lst\n",
    "(m) len(lst)\n",
    "\n",
    "#lst = [20, 1, -34, 40, -8, 60, 1, 3]\n",
    "\n",
    "lst = [20, 1, -34, 40, -8, 60, 1, 3]\n",
    "\n",
    "results = {\n",
    "    \"(a) lst\": lst,\n",
    "    \"(b) lst[0:3]\": lst[0:3],\n",
    "    \"(c) lst[4:8]\": lst[4:8],\n",
    "    \"(d) lst[4:33]\": lst[4:33],\n",
    "    \"(e) lst[-5:-3]\": lst[-5:-3],\n",
    "    \"(f) lst[-22:3]\": lst[-22:3],\n",
    "    \"(g) lst[4:]\": lst[4:],\n",
    "    \"(h) lst[:]\": lst[:],\n",
    "    \"(i) lst[:4]\": lst[:4],\n",
    "    \"(j) lst[1:5]\": lst[1:5],\n",
    "    \"(k) -34 in lst\": -34 in lst,\n",
    "    \"(l) -34 not in lst\": -34 not in lst,\n",
    "    \"(m) len(lst)\": len(lst)\n",
    "}\n",
    "for expr, val in results.items():\n",
    "    print(f\"{expr} = {val}\")\n",
    "\n",
    "(a) lst = [20, 1, -34, 40, -8, 60, 1, 3]\n",
    "(b) lst[0:3] = [20, 1, -34]\n",
    "(c) lst[4:8] = [-8, 60, 1, 3]\n",
    "(d) lst[4:33] = [-8, 60, 1, 3]\n",
    "(e) lst[-5:-3] = [40, -8]\n",
    "(f) lst[-22:3] = [20, 1, -34]\n",
    "(g) lst[4:] = [-8, 60, 1, 3]\n",
    "(h) lst[:] = [20, 1, -34, 40, -8, 60, 1, 3]\n",
    "(i) lst[:4] = [20, 1, -34, 40]\n",
    "(j) lst[1:5] = [1, -34, 40, -8]\n",
    "(k) -34 in lst = True\n",
    "(l) -34 not in lst = False\n",
    "(m) len(lst) = 8"
